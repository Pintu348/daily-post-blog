{
  "name": "handlebars-helper-ternary",
  "version": "1.0.0",
  "description": "Handlebars ternary helper",
  "main": "index.js",
  "scripts": {
    "coverage": "istanbul cover test/*.js",
    "coveralls": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "lint": "jshint .",
    "test": "tape test/*.js",
    "validate": "npm run lint && npm test && npm run coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jimf/handlebars-helper-ternary"
  },
  "keywords": [
    "handlebars",
    "helper",
    "ternary",
    "compare"
  ],
  "author": "Jim Fitzpatrick",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jimf/handlebars-helper-ternary/issues"
  },
  "homepage": "https://github.com/jimf/handlebars-helper-ternary",
  "devDependencies": {
    "coveralls": "^2.11.2",
    "handlebars": "^3.0.3",
    "istanbul": "^0.3.14",
    "jshint": "^2.7.0",
    "tape": "^4.0.0"
  }
}
